# API Keys Configuration
# Copy this file to .env and fill in your actual API keys

# OpenAI API (Required)
OPENAI_API_KEY=your_openai_api_key_here

# Job Search APIs (Choose one or both)
RAPIDAPI_KEY=your_rapidapi_key_here  # For JSearch API
ADZUNA_APP_ID=your_adzuna_app_id_here  # For Adzuna API (optional)
ADZUNA_APP_KEY=your_adzuna_app_key_here  # For Adzuna API (optional)

# Application Settings
DEBUG_MODE=True
LOG_LEVEL=INFO
